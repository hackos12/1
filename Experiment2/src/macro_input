MDT
1 INCR1	&FIRST,&SECOND=DATA9
2 A	1,#1
3 L	2,#2
4 MEND
5 INCR2	&ARG1,&ARG2
6 L	3,#1
7 ST	4,#2
8 MEND

MNT
1 INCR1 1	ALA: &FIRST &SECOND 
2 INCR2 5	ALA: &ARG1 &ARG2 

Intermediate code
PRG2	START
USING	*,BASE
INCR1	DATA1,DATA12
INCR2	DATA3,DATA4
FOUR	DC	F'4'
FIVE	DC	F'5'
BASE	EQU	8
TEMP	DS	'1'F
DROP	8
END